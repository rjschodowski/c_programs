#include <stdio.h>

//Reminder: the first [] is for rows, the second [] is for columns

//Also, An array can have more than two dimensions. For example, a[5][3][4] is an array that has 5 elements that each store 3 elements that each store 4 elements.

int main() {
    int a[3][5] = {
      {19, 12, 19, 8, 600},
      {0, 0, 0, 0, 0},
      {77, 23, 11, 1, 997}
      };
    
    int j, k;

    for(j=0; j<3; j++) {
      for(k=0; k<5; k++) {
        printf("%d\t", a[j][k]);
      }
        printf("\n");
    }
    
    return 0;
}
