int main() {
  int arr[] = {4, 8, 9, 2, 1, 3, 7, 6, 5};
  for(int i = 0; i < 9; i++)
    {
      if(arr[i] % 2 == 0)
      {
        printf("%d\n", arr[i]);
      }
      else if(arr[i] % 5 == 0)
      {
        printf("%d\n", arr[i]);
      }
      else
      {
        printf("Odd number\n");
      }
    }
  
}
